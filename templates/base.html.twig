<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Accueil
			{% endblock %}Gesflot</title>
		<!-- Icon -->
		<link
		rel="shortcut icon" href="{{ asset('assets/img/favicon.ico') }}" type="image/x-icon">

		<!-- Feuilles de styles -->
		<link rel="stylesheet" href="{{ asset('assets/css/bootstrap.min.css')}}"> {% block stylesheets %}{% endblock %}
		<link
		rel="stylesheet" href="{{ asset('assets/css/styles.css')}}">
		<!-- Fichiers Javascript -->
		<script src="{{ asset('assets/js/bootstrap.bundle.min.js')}} " defer></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/js/all.min.js" crossorigin="anonymous" defer></script>
		{% block javascripts %}{% endblock %}
		<script src="{{ asset('assets/js/scripts.js')}} " defer></script>
	</head>
	<body id="main">
		<div class="container-fluid">
			{% include "_partials/_nav.html.twig" %}
			{% block body %}{% endblock %}
			{% include "_partials/_footer.html.twig" %}
		</div>
	</body>
</html>
